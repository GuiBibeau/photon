{
  "name": "@photon/crypto",
  "version": "0.0.0",
  "description": "Cryptographic utilities for Photon SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    },
    "./keypair": {
      "types": "./dist/keypair.d.ts",
      "import": "./dist/keypair.mjs",
      "require": "./dist/keypair.js"
    },
    "./key-generation": {
      "types": "./dist/key-generation.d.ts",
      "import": "./dist/key-generation.mjs",
      "require": "./dist/key-generation.js"
    },
    "./hash": {
      "types": "./dist/hash.d.ts",
      "import": "./dist/hash.mjs",
      "require": "./dist/hash.js"
    },
    "./signing": {
      "types": "./dist/signing.d.ts",
      "import": "./dist/signing.mjs",
      "require": "./dist/signing.js"
    },
    "./compatibility": {
      "types": "./dist/compatibility.d.ts",
      "import": "./dist/compatibility.mjs",
      "require": "./dist/compatibility.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "size": "echo \"Size analysis not yet configured\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GuiBibeau/photon.git",
    "directory": "packages/crypto"
  },
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@photon/addresses": "workspace:*",
    "@photon/errors": "workspace:*"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.98.4",
    "tweetnacl": "^1.0.3"
  }
}
