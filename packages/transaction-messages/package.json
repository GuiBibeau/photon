{
  "name": "@photon/transaction-messages",
  "version": "0.0.0",
  "description": "Transaction message utilities for Photon SDK",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./create": {
      "types": "./dist/create.d.ts",
      "import": "./dist/create.mjs",
      "require": "./dist/create.js"
    },
    "./fee-payer": {
      "types": "./dist/fee-payer.d.ts",
      "import": "./dist/fee-payer.mjs",
      "require": "./dist/fee-payer.js"
    },
    "./instructions": {
      "types": "./dist/instructions.d.ts",
      "import": "./dist/instructions.mjs",
      "require": "./dist/instructions.js"
    },
    "./lifetime": {
      "types": "./dist/lifetime.d.ts",
      "import": "./dist/lifetime.mjs",
      "require": "./dist/lifetime.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "size": "echo \"Size analysis not yet configured\""
  },
  "dependencies": {
    "@photon/addresses": "workspace:*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GuiBibeau/photon.git",
    "directory": "packages/transaction-messages"
  },
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
