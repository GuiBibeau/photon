{
  "name": "@photon/signers",
  "version": "0.0.0",
  "description": "Signer utilities for Photon SDK",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "echo \"Linting not yet configured\"",
    "lint:fix": "echo \"Linting not yet configured\"",
    "format": "echo \"Formatting not yet configured\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "size": "echo \"Size analysis not yet configured\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GuiBibeau/photon.git",
    "directory": "packages/signers"
  },
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
