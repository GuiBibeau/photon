{
  "name": "@photon/integration",
  "version": "0.0.0",
  "description": "Integration tests and examples for Photon SDK packages",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev:addresses": "tsx watch src/examples/addresses.ts",
    "dev:rpc": "tsx watch src/examples/rpc.ts",
    "dev:send-sol": "tsx watch src/examples/send-sol.ts",
    "run:addresses": "tsx src/examples/addresses.ts",
    "run:rpc": "tsx src/examples/rpc.ts",
    "run:send-sol": "tsx src/examples/send-sol.ts",
    "run:send-sol:node": "pnpm build && node dist/examples/send-sol.cjs",
    "validator": "solana-test-validator",
    "validator:reset": "solana-test-validator --reset",
    "validator:logs": "solana logs",
    "airdrop": "solana airdrop 10",
    "build": "tsup --config tsup.config.ts",
    "test": "vitest",
    "test:integration": "vitest run test/integration",
    "test:integration:watch": "vitest watch test/integration",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage .turbo"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@photon/accounts": "workspace:*",
    "@photon/addresses": "workspace:*",
    "@photon/codecs": "workspace:*",
    "@photon/crypto": "workspace:*",
    "@photon/errors": "workspace:*",
    "@photon/rpc": "workspace:*",
    "@photon/rpc-subscriptions": "workspace:*",
    "@photon/signers": "workspace:*",
    "@photon/sysvars": "workspace:*",
    "@photon/transaction-messages": "workspace:*",
    "@photon/transactions": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.17.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "solana",
    "integration",
    "testing",
    "development",
    "examples"
  ]
}
