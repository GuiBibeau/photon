{
  "name": "@photon/spl-token",
  "version": "0.0.0",
  "private": true,
  "description": "SPL Token program support for Photon SDK",
  "author": "Photon SDK Contributors",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.mjs",
      "require": "./dist/constants.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    },
    "./instructions": {
      "types": "./dist/instructions.d.ts",
      "import": "./dist/instructions.mjs",
      "require": "./dist/instructions.js"
    },
    "./codecs/accounts": {
      "types": "./dist/codecs/accounts.d.ts",
      "import": "./dist/codecs/accounts.mjs",
      "require": "./dist/codecs/accounts.js"
    },
    "./codecs/instructions": {
      "types": "./dist/codecs/instructions.d.ts",
      "import": "./dist/codecs/instructions.mjs",
      "require": "./dist/codecs/instructions.js"
    },
    "./codecs/extensions": {
      "types": "./dist/codecs/extensions.d.ts",
      "import": "./dist/codecs/extensions.mjs",
      "require": "./dist/codecs/extensions.js"
    },
    "./codecs/tlv": {
      "types": "./dist/codecs/tlv.d.ts",
      "import": "./dist/codecs/tlv.mjs",
      "require": "./dist/codecs/tlv.js"
    },
    "./codecs/program-detection": {
      "types": "./dist/codecs/program-detection.d.ts",
      "import": "./dist/codecs/program-detection.mjs",
      "require": "./dist/codecs/program-detection.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "echo 'Skipping lint due to ESLint version conflict'",
    "lint:fix": "echo 'Skipping lint due to ESLint version conflict'",
    "format": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo coverage"
  },
  "dependencies": {
    "@photon/addresses": "workspace:*",
    "@photon/codecs": "workspace:*",
    "@photon/errors": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3",
    "tsup": "^8.3.5",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  }
}
