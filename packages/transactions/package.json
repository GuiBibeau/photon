{
  "name": "@photon/transactions",
  "version": "0.0.0",
  "description": "Transaction utilities for Photon SDK",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./compile": {
      "types": "./dist/compile.d.ts",
      "import": "./dist/compile.mjs",
      "require": "./dist/compile.js"
    },
    "./send": {
      "types": "./dist/send.d.ts",
      "import": "./dist/send.mjs",
      "require": "./dist/send.js"
    },
    "./serialize": {
      "types": "./dist/serialize.d.ts",
      "import": "./dist/serialize.mjs",
      "require": "./dist/serialize.js"
    },
    "./sign": {
      "types": "./dist/sign.d.ts",
      "import": "./dist/sign.mjs",
      "require": "./dist/sign.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "size": "echo \"Size analysis not yet configured\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GuiBibeau/photon.git",
    "directory": "packages/transactions"
  },
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@photon/addresses": "workspace:*",
    "@photon/codecs": "workspace:*",
    "@photon/crypto": "workspace:*",
    "@photon/signers": "workspace:*",
    "@photon/transaction-messages": "workspace:*"
  }
}
