{
  "name": "photon-sdk",
  "version": "0.0.0",
  "private": true,
  "description": "A lightweight, zero-dependency TypeScript SDK for Solana using Web Standards",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuiBibeau/photon.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch",
    "test:coverage": "pnpm -r test:coverage",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "pnpm -r format",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean",
    "dev": "pnpm -r --parallel dev",
    "size": "pnpm -r size"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
